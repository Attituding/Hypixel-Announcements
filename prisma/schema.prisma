generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Moderation_Information_and_Changes {
  id      String  @id
  title   String
  content String
  message String?

  @@map("Moderation Information and Changes")
}

model News_and_Announcements {
  id      String  @id
  title   String
  content String
  message String?

  @@map("News and Announcements")
}

model SkyBlock_Patch_Notes {
  id      String  @id
  title   String
  content String
  message String?

  @@map("SkyBlock Patch Notes")
}

model announcements {
  category    String @id
  channelID   String
  maxComments Int
  roleID      String
  url         String
}

model config {
  index              Int      @id @default(0)
  core               Boolean  @default(true)
  devMode            Boolean  @default(false)
  interval           Int      @default(210000)
  restRequestTimeout Int      @default(5000)
  retryLimit         Int      @default(3)
  ownerGuilds        String[] @default([])
  owners             String[] @default([])
}
